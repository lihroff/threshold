{
  "name": "@mozleo/node-starter",
  "version": "1.0.0",
  "description": "Personal boilerplate to kickstart creating a Node.js module/repo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tommy-White/node-starter.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --cache --fix --ext .js src *test*"
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "template",
    "starter"
  ],
  "author": {
    "name": "Tommy Lee",
    "email": "lihroff@icloud.com",
    "url": "https://fancylee.xyz"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.{js,jsx,ts,tsx,json,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/lihroff/node-starter/issues"
  }
}
